{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://192.168.1.36:8081/*" },
        { "url": "http://192.168.1.36:9000/*" }
      ]
    }
  ]
}